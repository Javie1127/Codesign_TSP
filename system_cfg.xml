<SystemCfg>
<SLS_Top_Cfg>
    <RAND_SEED>20781</RAND_SEED>
    <Num_PRI>8<Num_PRI>
    <Num_Range_Cell>32<Num_Range_Cell>    
    <NUM_DROPS>5</NUM_DROPS>
    <Bandwidth>
    <NUM_FADING_REALIZATIONS>200</NUM_FADING_REALIZATIONS>
    <isStatsPlotting>0</isStatsPlotting>
    <isLOPlotting>0</isLOPlotting>
    <isSave>2</isSave>    <!-- 
    SINR_eff_log=0 means old way of SINR logging, i.e., SINR_ave = averaging SINR over frequency bands.
    SINR_eff_log=1 means new way of logging, i.e., SINR is averaged in MI domain
    -->
    <SINR_eff_log>1</SINR_eff_log>
</SLS_Top_Cfg>
<FD>
    <isEnabled>1</isEnabled>
    <Num_DL_UE>4</Num_DL_UE>
    <Num_UL_UE>4</Num_UL_UE> 
    <Num_UE_Antennas>2</Num_UE_Antennas>
    <Num_BS_Antennas>8</Num_BS_Antennas>
    <BS_Noise_Figure>
</FD>
<Radar>
    <CarrierFrequency>2.6e9</CarrierFrequency>
    <FadingModel>EPA</FadingModel>
    <!-- 
    If RBEnable=1, SystemBandwidth indicates the number of RBs in system bandwidth; otherwise SystemBandwidth indicates bw in terms of Hz
    If SystemBandwidth indicates the number of RBs in system bandwidth, together with SubCarrierSpacing system bw in the unit of Hz can be automatically determined 
    If RBEnable=1, FadingGranularity indicates the number of RBs that experice the same channel fading; otherwise FadingGranularity is in terms of Hz
    -->
    <RBEnable>1</RBEnable>
    <SystemBandwidth>12</SystemBandwidth>
    <FadingGranularity>1</FadingGranularity>
    <SubCarrierSpacing>30e3</SubCarrierSpacing>
    <!--<SystemBandwidth>20e6</SystemBandwidth>
    <FadingGranularity>5e6</FadingGranularity>-->
    <K_Rician>0</K_Rician>
    <Nc_store>16</Nc_store>
    <Corr_Ant>
      <BS_Corr_L>0</BS_Corr_L>
      <BS_Corr_M>0.5</BS_Corr_M>
      <BS_Corr_H>1</BS_Corr_H>
      <UE_Corr_L>0</UE_Corr_L>
      <UE_Corr_M>0.5</UE_Corr_M>
      <UE_Corr_H>0.9</UE_Corr_H>

      <BS_Corr_L_Frac>1</BS_Corr_L_Frac>
      <BS_Corr_M_Frac>0</BS_Corr_M_Frac>
      <BS_Corr_H_Frac>0</BS_Corr_H_Frac>
      <UE_Corr_L_Frac>1</UE_Corr_L_Frac>
      <UE_Corr_M_Frac>0</UE_Corr_M_Frac>
      <UE_Corr_H_Frac>0</UE_Corr_H_Frac>
  </Corr_Ant>   
</ChannelSettings>
<gNB>
  <NumGNb_Antenna>2</NumGNb_Antenna>
  <NumGNb_Rows>1</NumGNb_Rows>
  <PTx>10</PTx>
  <PTxRelay>0</PTxRelay>
  <GTx_gNB_dB>0</GTx_gNB_dB>
  <GTx_Relay_dB>0</GTx_Relay_dB>
  <RRU>
     <Calib_Err>OFF</Calib_Err>
     <isChan_Err>0</isChan_Err>
     <Phase_Err>3</Phase_Err>
     <Mag_Err_dB>-30</Mag_Err_dB>
  </RRU>
  <BBU>
     <BeamForming>
         <BF_Algo>RZF</BF_Algo>
         <RZF_lambda>10</RZF_lambda>
         <txMode>COMP</txMode>
     </BeamForming>
     <Cell_Association>
         <isUE_Assoc_Enabled>0</isUE_Assoc_Enabled>
         <OverDimensionFactor>1</OverDimensionFactor>
         <AS_Metric>WSR</AS_Metric>
         <AS_Size_Max>4</AS_Size_Max>
         <O_complexity>0</O_complexity>
         <lkg_exponent>0.5</lkg_exponent>
         <max_lkg_allowed>0</max_lkg_allowed>
         <!-- 
         fixed_geographic_AS
         -->
         <clustering_type>single</clustering_type>
         <coord_silencing_post_clustering>0</coord_silencing_post_clustering>
     <coord_silencing_combinatorial_search_max_clusters>1</coord_silencing_combinatorial_search_max_clusters>
     <use_fast_fade_info_for_clustering>0</use_fast_fade_info_for_clustering>
     <clustering_init_off_num_candidates>8</clustering_init_off_num_candidates>
     <clustering_init_off_use_wsr_improvement_as_metric>0</clustering_init_off_use_wsr_improvement_as_metric>
     <clustering_init_off_use_metric_for_new_cluster>0</clustering_init_off_use_metric_for_new_cluster>
     <clustering_init_off_num_serving_cells>1</clustering_init_off_num_serving_cells>
     <Clustering_wsr_approx_params>
    <intra_cluster_interference_suppression_dB>30</intra_cluster_interference_suppression_dB>
     </Clustering_wsr_approx_params>
 </Cell_Association>
     <CSI_Source>SRS</CSI_Source>
     <BF_Alg>RZF</BF_Alg>
     <BF_Unit>2</BF_Unit>
  </BBU>
  <NetworkCFG>
     <SRS_PERIODICITY>5</SRS_PERIODICITY>
  </NetworkCFG>
  <BLER_Target>0.1</BLER_Target>
  <Rate_Control>
      <RankSelEnable>1</RankSelEnable>
      <UserSelEnable>1</UserSelEnable>
      <MaximumRank>2</MaximumRank>
      <DeltaUp>0.9</DeltaUp>
      <R1Priority>0</R1Priority>
  </Rate_Control>    
</gNB>
<UE>
  <NumUE_Antenna>2</NumUE_Antenna>
  <NumUE_Antenna_Observed>2</NumUE_Antenna_Observed>
  <NumMIMOLayers>1</NumMIMOLayers>
  <SNRdB_Cap>60</SNRdB_Cap>
  <gap2Cap>3</gap2Cap>
  <GRx_UE_dB>0</GRx_UE_dB>
  <NoiseFigure>5</NoiseFigure>
  <ChannelEst>Genie</ChannelEst>
  <Demapper>MMSE_IRC</Demapper>
  <rate_per_layer_cap>8</rate_per_layer_cap>
</UE>
<Scheduler>
  <Enabled>1</Enabled>
  <FDM_Enabled>0</FDM_Enabled>
  <FDM_scheduler_id>1</FDM_scheduler_id>
  <FD_PF_strategy_id>2</FD_PF_strategy_id>
  <SchedulingBW>2</SchedulingBW>
  <scheduling_window>80</scheduling_window>
  <max_number_of_ue_to_schedule_per_cluster>8</max_number_of_ue_to_schedule_per_cluster>
  <Cluster_Init_Range_Expansion_Margin_dB>0</Cluster_Init_Range_Expansion_Margin_dB>
<force_rank_1_for_approx_wsr_calculations>0</force_rank_1_for_approx_wsr_calculations></Scheduler>
<Traffic_model>
   <Enabled>1</Enabled>
   <model_index>3</model_index>
   <fixed_arrival_rate_enabled>0</fixed_arrival_rate_enabled>
   <fixed_arrival_rate>1/(5e-3)</fixed_arrival_rate>
   <fixed_file_size>1</fixed_file_size>
   <retran_gap>2</retran_gap>
   <file_size>5</file_size>
   <file_sizes>0.25:0.05:0.45</file_sizes> <!--file sizes are in Mbytes-->
   <arrival_rates>1e3./[20:5:40]</arrival_rates><!--arrival rates in packets/s-->
   <!-- <pair_probs>[0.5 1.2 0.9 0.7 1.7]/5</pair_probs>
   <pair_probs>[1.7 0.7 0.9 1.2 0.5]/5</pair_probs>
   <pair_probs>[0.5 1.7 0.7 0.9 1.2]/5</pair_probs>
   <pair_probs>[0.5 1.2 0.9 0.7 1.7]/5</pair_probs>
    -->
   <pair_probs>[1.7 0.7 0.9 1.2 0.5]/5</pair_probs>
    <!--<pair_probs>[0.5 1.7 0.7 0.9 1.2]/5</pair_probs>-->
    <!--<pair_probs>[1.2 0.5 1.7 0.7 0.9]/5</pair_probs>-->
    <!--<pair_probs>[0.9 1.2 0.5 1.7 0.7]/5</pair_probs>-->
    <!--<pair_probs>[0.7 0.9 1.2 0.5 1.7]/5</pair_probs>-->
   <file_dropping_enabled>1</file_dropping_enabled>
   <file_drop_condition>Inf</file_drop_condition>
</Traffic_model>
</SystemCfg>
